<% extend("../_base/layout") %>

<main>
	<h1>Welcome <%= user.name %></h1>
	<input data-token="<%= token %>" type="hidden" value="<%= token %>">
	<input data-login="<%= user.login %>" type="hidden" value="<%= user.login %>">
	<input data-user="<%= user.name %>" type="hidden" value="<%= user.name %>">

	<table id="ranking" cellspacing="0">
		<thead>
			<th>#</th>
			<th>User</th>
			<th>Commits</th>
		</thead>
		<tbody></tbody>
	</table>
</main>
<script src="https://cdn.socket.io/socket.io-1.3.5.js"></script>
<script src="<%= assetFingerprint('/js/app.js') %>" type="text/javascript"></script>